<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
 <table align="center" border="2">
        <thead>
            <tr >
                <th bgcolor="lightgreen">Sr. No.</th>
                <th bgcolor="lightgreen">Text Data</th>
                <th bgcolor="lightgreen">Number Data</th>
            </tr>
        </thead>
        <tbody>
            <%
                for (int i = 0; i < 10; i++) {
            %>
            <tr >
                <td bgcolor="lightblue" align="center"><%=i%></td>
                <td bgcolor="lightblue" align="center">This is text data <%=i%></td>
                <td bgcolor="lightblue" align="center"><%=i * i%></td>
            </tr>
            <%
                }
            %>
        </tbody>
</table>