<title>InnoDitor Flash Upload</title>

<script language="javascript" type="text/javascript" src="./js/browserlanguage.js"></script>
<script language="javascript" type="text/javascript">
//<![CDATA[
// language force setting - english
g_browserCHK.language = "en";
//]]>
</script>
<script language="javascript" type="text/javascript" src="./js/uploadflash_u.js"></script>
<script language="javascript" type="text/javascript">
//<![CDATA[
// customer setting variable
// if this example apply...
var g_strUploadPath_Flash = "/z_experience/upload_tran_u.asp";
//]]>
</script>

<script language="javascript" type="text/javascript">
//<![CDATA[
function fnFlashSend()
{
	var strFlashURL = "";

	if(1 == g_nSelectedTabIndex)
	{
		if("" == g_objUploadObjectProp.upload_url)
		{
			alert(res_warning_msg[4]);
			return;
		}

		strFlashURL = g_objUploadObjectProp.upload_url;
	}
	else
	{
		if("" == g_objLoadURLObjectProp.flash_url)
		{
			alert(res_warning_msg[5]);
			return;
		}

		strFlashURL = g_objLoadURLObjectProp.flash_url;
	}


	if("" != document.getElementById("txtWidth").value)
	{
		if(isNaN(document.getElementById("txtWidth").value))
		{
			alert(res_warning_msg[6]);
			document.getElementById("txtWidth").focus();
			return;
		}
	}

	if("" != document.getElementById("txtHeight").value)
	{
		if(isNaN(document.getElementById("txtHeight").value))
		{
			alert(res_warning_msg[6]);
			document.getElementById("txtHeight").focus();
			return;
		}
	}

	if("" != document.getElementById("txtBorder").value)
	{
		if(isNaN(document.getElementById("txtBorder").value))
		{
			alert(res_warning_msg[6]);
			document.getElementById("txtBorder").focus();
			return;
		}
	}

	if("" != document.getElementById("txtHSpace").value)
	{
		if(isNaN(document.getElementById("txtHSpace").value))
		{
			alert(res_warning_msg[6]);
			document.getElementById("txtHSpace").focus();
			return;
		}
	}

	if("" != document.getElementById("txtVSpace").value)
	{
		if(isNaN(document.getElementById("txtVSpace").value))
		{
			alert(res_warning_msg[6]);
			document.getElementById("txtVSpace").focus();
			return;
		}
	}

	var arrReturnValue = new Array();
	arrReturnValue[0] = strFlashURL;
	arrReturnValue[1] = document.getElementById("txtWidth").value;
	arrReturnValue[2] = document.getElementById("txtHeight").value;
	arrReturnValue[3] = document.getElementById("selAlign").value;
	arrReturnValue[4] = document.getElementById("txtBorder").value;
	arrReturnValue[5] = document.getElementById("txtHSpace").value;
	arrReturnValue[6] = document.getElementById("txtVSpace").value;

	if(g_browserCHK.wk || g_browserCHK.op)
	{
		// Chrome  / Safari / Opera
		opener.fnSetFlashReturnValue(arrReturnValue);
	}
	else if(g_browserCHK.ie || g_browserCHK.ff)
	{
		// IE  /  FireFox
		window.returnValue = arrReturnValue;
	}

	window.close();
}
//]]>
</script>
</head>

<body onload="javascript:fnAdjustPage();">

<div style="width:598px;height:530px;border:1px solid #e2e2e2;">
	<div style="margin-left:16px;margin-right:16px;">

		<div style="margin-top:10px;"><img id="res_image_0" alt="" src="./image/blank.gif" /></div>

		<!-- Tab Area -->
		<div style="height:27px;margin-top:12px;border-bottom:1px solid #d2d2d2;">
			<div style="display:inline;">
				<a href="javascript:fnTabClick(1);"><img id="imgTab_1" alt="upload tab" src="./image/blank.gif" onmouseover="javascript:fnTabMouseOver(this,1);" onmouseout="javascript:fnTabMouseOut(this,1);" /></a>
			</div>
			<div style="display:inline;">
				<a href="javascript:fnTabClick(2);"><img id="imgTab_2" alt="link tab" src="./image/blank.gif" onmouseover="javascript:fnTabMouseOver(this,2);" onmouseout="javascript:fnTabMouseOut(this,2);" /></a>
			</div>
		</div>

		<div style="margin-top:2px;">

			<!-- link, file, upload button UI Set Area -->
			<div style="width:550px;">
				<div style="display:inline;float:left;vertical-align:top;">
					<input type="text" title="flash url" id="txtFlashPath" name="txtFlashPath" class="form" style="width:398px" maxlength="512" value="" />
				</div>
				<div style="display:inline;">

					<!-- file, upload button Area -->
					<div id="flashUploadArea">
						<div style="width:65px;height:20px;margin-left:7px;display:inline;float:left;">
							<form name="frmUpload" method="post" action="" enctype="multipart/form-data" onsubmit="return false">
								<div class="UploadFileContainer"><input type="file" title="file" id="fileUpload" name="fileUpload" class="UploadFile" onfocus="javascript:fnFired_Focus_File(this);" onblur="javascript:fnFired_Blur_File(this);" style="cursor:pointer;" onchange="javascript:fnChangeFile(this.value);" /></div>
								<input type="hidden" name="hdnUploadType" value="2" />
							</form>
						</div>
						<div style="display:inline;">
							<a href="javascript:fnUploadFile();"><img id="res_image_1" alt="upload" src="./image/blank.gif" /></a>
						</div>
					</div>

					<!-- link Area -->
					<div id="flashLoadURLArea" style="display:none;">
						<div style="width:65px;height:22px;margin-left:7px;display:inline;float:left;">
							<a href="javascript:fnLoadURLFlash();"><img id="res_image_2" alt="load url" src="./image/blank.gif" /></a>
						</div>
					</div>

				</div>
			</div>

			<div style="height:2px;"></div>

			<!-- flash preview Area(main) -->
			<div style="display:inline;float:left;width:410px;clear:both;">
				<table border="0" cellpadding="0" cellspacing="0">
				<tr>
					<td id="flash_main" align="center" style="width:400px;height:410px;border:1px solid #c4cad1;background-color:#ffffff;background-image:url(./image/editwin/pop_upload_thumnail_bg.gif);">
						<img id="res_image_3" alt="upload" src="./image/blank.gif" />
					</td>
				</tr>
				</table>
			</div>
			<div style="display:inline;float:left;width:2px;"></div>
			<div style="display:inline;float:left;vertical-align:top;">

				<div style="margin-top:6px;text-align:center;">
					<span id="res_item_0"></span>
				</div>

				<!-- width, height Area -->
				<div style="margin-top:10px;">
					<div>
						<b id="res_item_1"></b>
					</div>
					<div style="margin-top:4px;clear:both;">
						<div id="res_item_2" style="display:inline;float:left;width:70px;"></div>
						<div style="display:inline;float:left;vertical-align:top;margin-top:-4px;">
							<input id="txtWidth" title="Width" name="txtWidth" type="text" class="form" style="width:40px;text-align:right" maxlength="5" value="300" />
						</div>
						<div style="display:inline;">(px)</div>
					</div>
					<div style="margin-top:4px;clear:both;">
						<div id="res_item_3" style="display:inline;float:left;width:70px;"></div>
						<div style="display:inline;float:left;vertical-align:top;margin-top:-4px;">
							<input id="txtHeight" title="Height" name="txtHeight" type="text" class="form" style="width:40px;text-align:right" maxlength="5" value="300" />
						</div>
						<div style="display:inline;">(px)</div>
					</div>
				</div>

				<!-- align Area -->
				<div style="margin-top:10px;">
					<div>
						<b id="res_item_4"></b>
					</div>
					<div style="margin-top:4px;">
						<select id="selAlign" title="Align" name="selAlign" class="form" style="width:124px">
							<option value=""></option>
							<option value="left"></option>
							<option value="middle"></option>
							<option value="right"></option>
							<option value="top"></option>
							<option value="bottom"></option>
							<option value="baseline"></option>
							<option value="texttop"></option>
							<option value="absmiddle"></option>
							<option value="absbottom"></option>
						</select>
					</div>
				</div>

				<!-- border Area -->
				<div style="margin-top:10px;">
					<div>
						<b id="res_item_5"></b>
					</div>
					<div style="margin-top:4px;">
						<div id="res_item_6" style="display:inline;float:left;width:90px;"></div>
						<div style="display:inline;float:left;vertical-align:top;margin-top:-4px;">
							<input id="txtBorder" title="Border" name="txtBorder" type="text" class="form" style="width:20px;text-align:right" maxlength="2" value="0" />
						</div>
						<div style="display:inline;">(px)</div>
					</div>
				</div>

				<!-- hspace/vspace Area -->
				<div style="margin-top:10px;">
					<div>
						<b id="res_item_7"></b>
					</div>
					<div style="margin-top:4px;clear:both;">
						<div id="res_item_8" style="display:inline;float:left;width:70px;"></div>
						<div style="display:inline;float:left;vertical-align:top;margin-top:-4px;">
							<input id="txtHSpace" title="Horizontal" name="txtHSpace" type="text" class="form" style="width:20px;text-align:right" maxlength="2" value="0" />
						</div>
						<div style="display:inline;vertical-align:top;">(px)</div>
					</div>
					<div style="margin-top:4px;clear:both;">
						<div id="res_item_9" style="display:inline;float:left;width:70px;"></div>
						<div style="display:inline;float:left;vertical-align:top;margin-top:-4px;">
							<input id="txtVSpace" title="Vertical" name="txtVSpace" type="text" class="form" style="width:20px;text-align:right" maxlength="2" value="0" />
						</div>
						<div style="display:inline;vertical-align:top;">(px)</div>
					</div>
				</div>

				<!-- apply,cancel Area -->
				<div style="margin-top:20px;text-align:center;">
					<div style="display:inline;">
						<a href="javascript:fnFlashSend();"><img id="res_image_4" alt="apply" src="./image/blank.gif" /></a>
					</div>
					<div style="display:inline;">
						<a href="javascript:window.close();"><img id="res_image_5" alt="cancel" src="./image/blank.gif" /></a>
					</div>
				</div>

			</div>
		</div>
	</div>
</div>

<!-- Upload Form Action Area : Start -->
<iframe name="ifrmUpload" src="" style="display:none" width="0" height="0" frameborder="0" marginheight="0" marginwidth="0"></iframe>
<!-- Upload Form Action Area : End -->

<div id="uploadStatus" style="display:none">
	<div class="UploadStatus" style="top:0px;left:0px;width:600px;height:230px"></div>
	<div class="UploadStatus" style="top:230px;left:0px;width:160px;height:80px"></div>
	<div class="UploadStatusBar" style="top:230px;left:160px;width:280px;height:80px"><img id="res_image_6" alt="upload status" src="./image/editwin/ko/pop_upload_status.gif" /></div>
	<div class="UploadStatus" style="top:230px;left:440px;width:160px;height:80px"></div>
	<div class="UploadStatus" style="top:310px;left:0px;width:600px;height:220px"></div>
</div>

</body> 
</html>
