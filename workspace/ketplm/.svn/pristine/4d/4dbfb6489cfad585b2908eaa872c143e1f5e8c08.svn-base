package e3ps.edm.clients.batch;

import java.io.Serializable;

public class EPMCodeData implements Serializable {
	/**
	 * 
	 */
	private static final long serialVersionUID = 5658650170886073069L;
	
	
	public String m_type;
	public String m_value;
	public String m_name;
	public String m_oid;
	
	public EPMCodeData(String type, String value, String name, String oid) {
		m_type = type;
		m_value = value;
		m_name = name;
		m_oid = oid;
	}
	
	public String getType() {
		return m_type;
	}
	
	public String getName() {
		return m_name;
	}
	
	public String getValue() {
		return m_value;
	}
	
	public String getOid() {
		return m_oid;
	}
	
	public String toString() {
		return m_name;
	}
}
