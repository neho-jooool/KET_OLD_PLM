            <required>true</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <name>partClazCode</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <name>isEsse</name>
            <required>true</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
    </tag>
    <tag>
        <name>manufPlace</name>
        <tag-class>ext.ket.shared.tld.ManufacturePlaceTag</tag-class>
        <body-content>empty</body-content>
        <attribute>
