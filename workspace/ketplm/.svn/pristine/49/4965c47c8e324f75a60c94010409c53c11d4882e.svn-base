        <attribute>
            <name>esse</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <name>esseLabel</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <name>useNullValue</name>
            <required>false</required>
            <rtexprvalue>false</rtexprvalue>
        </attribute>
	</tag>
	<tag>
        <name>codeValueByCode</name>
        <tag-class>ext.ket.shared.tld.CodeValueByCodeTag</tag-class>
        <body-content>empty</body-content>
        <attribute>
            <name>codeType</name>
            <required>true</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <name>code</name>
            <required>true</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
    </tag>
	<tag>
        <name>partUnit</name>
        <tag-class>ext.ket.shared.tld.SelectPartUnitTag</tag-class>
        <body-content>empty</body-content>
        <attribute>
            <name>selectCode</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <name>htmlAttribute</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
    </tag>
        <tag>
        <name>partSpecialEsse</name>
        <tag-class>ext.ket.shared.tld.PartSpecialEsseTag</tag-class>
