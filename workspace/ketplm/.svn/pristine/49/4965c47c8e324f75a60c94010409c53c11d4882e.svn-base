    <function>
      <description>is Admin check</description>
      <name>isAdmin</name>
      <function-class>e3ps.common.util.CommonUtil</function-class>
      <function-signature>
           java.lang.Boolean isAdmin()
      </function-signature>
    </function>
    
    <function>
      <description>is Cost Admin check</description>
      <name>isCostAdmin</name>
      <function-class>e3ps.common.util.CommonUtil</function-class>
      <function-signature>
           java.lang.Boolean isCostAdmin()
      </function-signature>
    </function>
</taglib>