            <required>true</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
    </tag>
    <tag>
        <name>partNoMulti</name>
        <tag-class>ext.ket.shared.tld.PartNoMultiTag</tag-class>
        <body-content>empty</body-content>
        <attribute>
            <name>code</name>
            <required>true</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
    </tag>
    <tag>
        <name>materialInfo</name>
        <tag-class>ext.ket.shared.tld.MaterialInfoTag</tag-class>
