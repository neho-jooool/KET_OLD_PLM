@charset "UTF-8";
.info{border-top:2px solid #779cb4;width:100%;table-layout:fixed;}

.WP60{width:60% !important;}
.WP70{width:70% !important;}
.WP80{width:80% !important;}
.WP90{width:90% !important;}
.WP95{width:95% !important;}
.WP100{width:100% !important;}

.W50{width:50px !important;}
.W60{width:60px !important;}
.W80{width:80px !important;}
.W90{width:90px !important;}
.W100{width:100px !important;}
.W150{width:150px !important;}
.W200{width:200px !important;}
.W300{width:300px !important;}

.fixed{table-layout:fixed;}
.webEditor{overflow-x:auto;overflow-y:hidden;width:100%;}
.ui-datepicker-trigger{cursor:pointer;}
.cursor-p{ cursor:pointer; }
.cursor-d{ cursor:default; }

.table-info{width:100%;border-top:2px solid #779CB4;}
.table-info thead td{
	border-bottom-style: solid;
    border-bottom-color: #b7d1e2;
    border-bottom-width: 1px;
    border-right-style: solid;
    border-right-color: #b7d1e2;
    border-right-width: 1px;
    background-color: #e2edf4;
    height: 25px;
    padding-top: 2px;
    color: #34738d;
    font-family: NanumGothic, "나눔고딕", Nanumgo, Dotum;
    font-family: NanumBold !important;
    font-size: 13px;
    text-align: center;
}
.table-info tbody .td{
	border-bottom-width: 1px;
    border-bottom-style: solid;
    border-bottom-color: #e5e4e4;
    border-right-width: 1px;
    border-right-style: solid;
    border-right-color: #e5e4e4;
    background-color: #F7F7F7;
    height: 22px;
    color: #575757;
    padding-top: 2px;
    padding-left: 5px;
    font-family: NanumGothic, "나눔고딕", Nanumgo, Dotum;
    font-size: 12px;
}
.left{text-align:left !important;}
.center{text-align:center !important;}
.right{text-align:right !important;}
.top{vertical-align:top !important;}
.bottom{vertical-align:bottom !important;}
