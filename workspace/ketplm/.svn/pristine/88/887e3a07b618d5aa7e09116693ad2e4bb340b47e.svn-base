package e3ps.bom.command.comparebom.staticcompare;

import e3ps.bom.common.component.*;

/**
 * class Name  : StaticCompareResultData
 * Description : Static비교의 결과 data를 갖고 있는 object
 */
public class StaticCompareResultData
{
	private String firstFlagStr = "";
	private String secondFlagStr = "";
	private String codeStr = "";
	private String designatorStr = "";
	private String itemCodeStr = "";

	private BOMAssyComponent sourceAssyComponent = new BOMAssyComponent();
	private BOMAssyComponent targetAssyComponent = new BOMAssyComponent();

	private BOMDesignatorComponent sourceDesignatorComponent = new BOMDesignatorComponent();
	private BOMDesignatorComponent targetDesignatorComponent = new BOMDesignatorComponent();

	public StaticCompareResultData()
	{
	}

	// SET METHOD
	public void setFirstFlag(String firstFlagStr)
	{
		this.firstFlagStr = firstFlagStr;
	}

	public void setSecondFlag(String secondFlagStr)
	{
		this.secondFlagStr = secondFlagStr;
	}
	
	public void setCode(String codeStr)
	{
		this.codeStr = codeStr;
	}

	public void setDesignatorNo(String designatorStr)
	{
		this.designatorStr = designatorStr;
	}

	public void setItemCode(String itemCodeStr)
	{
		this.itemCodeStr = itemCodeStr;
	}

	public void setSourceAssyComponent(BOMAssyComponent sourceAssyComponent)
	{
		this.sourceAssyComponent = sourceAssyComponent;
	}

	public void setTargetAssyComponent(BOMAssyComponent targetAssyComponent)
	{
		this.targetAssyComponent = targetAssyComponent;
	}

	public void setSourceDesignatorComponent(BOMDesignatorComponent sourceDesignatorComponent)
	{
		this.sourceDesignatorComponent = sourceDesignatorComponent;
	}

	public void setTargetDesignatorComponent(BOMDesignatorComponent targetDesignatorComponent)
	{
		this.targetDesignatorComponent = targetDesignatorComponent;
	}

	// GET METHOD
	public String getFirstFlag()
	{
		return firstFlagStr;
	}

	public String getSecondFlag()
	{
		return secondFlagStr;
	}
	
	public String getCode()
	{
		return codeStr;
	}

	public String getDesignatorNo()
	{
		return designatorStr;
	}

	public String getItemCode()
	{
		return itemCodeStr;
	}

	public BOMAssyComponent getSourceAssyComponent()
	{
		return sourceAssyComponent;
	}

	public BOMAssyComponent getTargetAssyComponent()
	{
		return targetAssyComponent;
	}

	public BOMDesignatorComponent getSourceDesignatorComponent()
	{
		return sourceDesignatorComponent;
	}

	public BOMDesignatorComponent getTargetDesignatorComponent()
	{
		return targetDesignatorComponent;
	}
}
