	td{font-size:12px; color:#747474;}
	.tb{border-top:1px solid #48b0c0; border-bottom: 1px solid #c0d7d9;}
	
	.tb th{
	background:#becfd5;
	border-bottom:1px solid #799fac;
	padding:5px 5px; color:#000000;font-size:12px; font-weight:bold;}
	
	.tb td{
	border-bottom:1px solid #c0d7d9; 
	border-right:1px solid #c0d7d9;
	padding:3px 5px;
	}
	.tb td.tdEnd{border-right:0;}
	.tb{border-collapse: collapse;border: solid 0px gray;}
	
	
	.modifyRadio{vertical-align:-3.0px;}
	
	a{text-decoration:none;color:#747474}
	a:hover{text-decoration:none; color:#747474;}