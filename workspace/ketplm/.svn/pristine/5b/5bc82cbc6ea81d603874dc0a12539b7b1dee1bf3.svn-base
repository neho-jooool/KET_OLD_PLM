	<servlet>
		<description>SpecEditorServlet</description>
		<servlet-name>com.ptc.ddl.servlet.speced.SpecEditorServlet</servlet-name>
		<servlet-class>com.ptc.ddl.servlet.speced.SpecEditorServlet</servlet-class>
	</servlet>

	<servlet>
