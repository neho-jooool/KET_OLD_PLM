package e3ps.bom.common;

public class ItemTableData 
{
    private String itemCode;
    private String desc;
    private String uom;
	private String userItemType;
	private String supplyType;
	private String status;
	private String defaultunit;
	private String uitStr;
	private String userNmStr;
	private String startDate;
	private String statusKrStr;

    public ItemTableData() 
	{
        itemCode = "";
        desc = "";
        uom = "";
		userItemType = "";
		supplyType = "";
		status = "";
		defaultunit = "";
		uitStr = "";
		userNmStr = "";
		startDate = "";
		statusKrStr = "";
    }

    public void setItemCode(String s) 
	{
        if (s != null)
            itemCode = s;
    }

    public void setDesc(String s) 
	{
        if (s != null)
            desc = s;
    }
    
    public void setDefaultunit(String s) 
	{
        if (s != null)
        	defaultunit = s;
    }

    public void setUom(String s) 
	{
        if (s != null)
            uom = s;
    }

    public void setUserItemType(String s) 
	{
        if (s != null)
            userItemType = s;
    }

    public void setSupplyType(String s) 
	{
        if (s != null)
            supplyType = s;
    }

    public void setStatus(String s) 
	{
        if (s != null)
            status = s;
    }
    
    public void setUitStr(String s)
    {
        if(s != null)
		{
            uitStr = s;
		}
    }
    
    public void setUserNmStr(String s)
    {
        if(s != null)
		{
        	userNmStr = s;
		}
    }
    public void setStartDate(String s)
    {
        if(s != null)
		{
        	startDate = s;
		}
    }
    public void setStatusKrStr(String s)
    {
        if(s != null)
		{
        	statusKrStr = s;
		}
    }
    
    public String getItemCode() 
	{
        return itemCode;
    }

    public String getDesc() 
	{
        return desc;
    }
    
    public String getDefaultunit() 
	{
        return defaultunit;
    }

   
    public String getUom() 
	{
        return uom;
    }

    public String getUserItemType() 
	{
        return userItemType;
    }

    public String getSupplyType() 
	{
        return supplyType;
    }

    public String getStatus() 
	{
        return status;
    }
    public String getUitStr()
    {
        return uitStr;
    }
    
    public String getUserNmStr()
    {
        return userNmStr;
    }
    public String getStartDate()
    {
        return startDate;
    }
    public String getStatusKrStr()
    {
        return statusKrStr;
    }
 
}
