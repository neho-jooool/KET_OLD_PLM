package ext.ket.edm.stamping.oxm;

import javax.xml.bind.annotation.XmlAccessType;
import javax.xml.bind.annotation.XmlAccessorType;
import javax.xml.bind.annotation.XmlRootElement;
import javax.xml.bind.annotation.adapters.XmlJavaTypeAdapter;

import org.apache.commons.lang.builder.ToStringBuilder;
import org.apache.commons.lang.builder.ToStringStyle;

@XmlAccessorType(XmlAccessType.FIELD)
@XmlRootElement(name = "root")
public class DrawingDeployRequest {

    @XmlJavaTypeAdapter(value = CDATAAdapter.class)
    private String fileSeperator;
    @XmlJavaTypeAdapter(value = CDATAAdapter.class)
    private String foldDefaultPath;
    private String foldYear;
    private String foldMonth;
    private String foldDay;
    private String foldRequestNo;
    private String executeNo;

    private String transNxFileSuffix;
    private String transCreoFileSuffix;
    private String transLogFileExt;
    private String transErrFileSuffix;

    private String stampFileSuffix;
    private String stampLogFileExt;
    private String stampErrFileSuffix;
    @XmlJavaTypeAdapter(value = CDATAAdapter.class)
    private String stampCompanyName;

    private RequestDrawingList requestDrawingList;

    public void setFileSeperator(String fileSeperator) {
	this.fileSeperator = fileSeperator;
    }

    public void setFoldDefaultPath(String foldDefaultPath) {
	this.foldDefaultPath = foldDefaultPath;
    }

    public void setFoldYear(String foldYear) {
	this.foldYear = foldYear;
    }

    public void setFoldMonth(String foldMonth) {
	this.foldMonth = foldMonth;
    }

    public void setFoldDay(String foldDay) {
	this.foldDay = foldDay;
    }

    public void setFoldRequestNo(String foldRequestNo) {
	this.foldRequestNo = foldRequestNo;
    }

    public void setExecuteNo(String executeNo) {
	this.executeNo = executeNo;
    }

    public void setTransNxFileSuffix(String transNxFileSuffix) {
	this.transNxFileSuffix = transNxFileSuffix;
    }

    public void setTransCreoFileSuffix(String transCreoFileSuffix) {
	this.transCreoFileSuffix = transCreoFileSuffix;
    }

    public void setTransLogFileExt(String transLogFileExt) {
	this.transLogFileExt = transLogFileExt;
    }

    public void setTransErrFileSuffix(String transErrFileSuffix) {
	this.transErrFileSuffix = transErrFileSuffix;
    }

    public void setStampFileSuffix(String stampFileSuffix) {
	this.stampFileSuffix = stampFileSuffix;
    }

    public void setStampLogFileExt(String stampLogFileExt) {
	this.stampLogFileExt = stampLogFileExt;
    }

    public void setStampErrFileSuffix(String stampErrFileSuffix) {
	this.stampErrFileSuffix = stampErrFileSuffix;
    }

    public void setStampCompanyName(String stampCompanyName) {
	this.stampCompanyName = stampCompanyName;
    }

    public void setRequestDrawingList(RequestDrawingList requestDrawingList) {
	this.requestDrawingList = requestDrawingList;
    }

    public String getFileSeperator() {
	return fileSeperator;
    }

    public String getFoldDefaultPath() {
	return foldDefaultPath;
    }

    public String getFoldYear() {
	return foldYear;
    }

    public String getFoldMonth() {
	return foldMonth;
    }

    public String getFoldDay() {
	return foldDay;
    }

    public String getFoldRequestNo() {
	return foldRequestNo;
    }

    public String getExecuteNo() {
	return executeNo;
    }

    public String getTransNxFileSuffix() {
	return transNxFileSuffix;
    }

    public String getTransCreoFileSuffix() {
	return transCreoFileSuffix;
    }

    public String getTransLogFileExt() {
	return transLogFileExt;
    }

    public String getTransErrFileSuffix() {
	return transErrFileSuffix;
    }

    public String getStampFileSuffix() {
	return stampFileSuffix;
    }

    public String getStampLogFileExt() {
	return stampLogFileExt;
    }

    public String getStampErrFileSuffix() {
	return stampErrFileSuffix;
    }

    public String getStampCompanyName() {
	return stampCompanyName;
    }

    public RequestDrawingList getRequestDrawingList() {
	return requestDrawingList;
    }

    public String toString() {
	return ToStringBuilder.reflectionToString(this, ToStringStyle.MULTI_LINE_STYLE);
    }
}
