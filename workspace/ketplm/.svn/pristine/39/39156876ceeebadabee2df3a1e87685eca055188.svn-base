package ext.ket.cost.util;

import ext.ket.cost.entity.CostPart;
import ext.ket.cost.entity.manage.BomEditorACLFromType;

public class NessaryCheckResult {
    private boolean ok;
    private CostPart part;
    private int row;
    private BomEditorACLFromType aclColumnInfo;

    public boolean isOk() {
        return ok;
    }

    public void setOk(boolean ok) {
        this.ok = ok;
    }

    public CostPart getPart() {
        return part;
    }

    public void setPart(CostPart part) {
        this.part = part;
    }

    public int getRow() {
        return row;
    }

    public void setRow(int row) {
        this.row = row;
    }

    public BomEditorACLFromType getAclColumnInfo() {
        return aclColumnInfo;
    }

    public void setAclColumnInfo(BomEditorACLFromType aclColumnInfo) {
        this.aclColumnInfo = aclColumnInfo;
    }

}
