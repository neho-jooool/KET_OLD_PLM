@charset "utf-8";

/* font */

@font-face{
font-family: Nanumgo;
src: url(../font/NanumGothic.eot);src: local(※), url(../font/NanumGothic.woff) format('woff')
}

@font-face{
font-family: NanumBold;
src: url(../font/NanumGothicBold.eot);src: local(※), url(../font/NanumGothicBold.woff) format('woff')
}

/* Common Definition */

html, body,div, span,dl, dt, dd, ul, ol, li,h1, h2, h3, h4, h5, h6,blockquote, p, address, pre, cite,form, fieldset, input, textarea, select, label,table, th, td {margin:0;padding:0;border:0;outline:0}
h1, h2, h3, h4, h5, h6 {font-size:100%;font-weight:normal} 
fieldset, img, abbr,acronym {border:0 none} 
ol, ul {list-style:none}
table {border-collapse: collapse;border-spacing:0;border:0 none}
caption, th, td {text-align:left;font-weight: normal}
address, caption, strong, em, cite {font-weight:normal;font-style:normal}
ins {text-decoration:none;}
del {text-decoration:line-through}
blockquote:before, blockquote:after, q:before, q:after {content:""}
blockquote,q {quotes:"" ""}
hr {display:none}
legend, caption {position:absolute;overflow:hidden;width:1px;height:1px;text-indent:-5000em;font-size:0;line-height:0}
html {height:100%;}
span, input, select {vertical-align:middle}
body {width:100%;height:100%;font-size:75%;font-family: NanumGothic, "나눔고딕", Nanumgo, NanumBold, Malgun gothic, "맑은고딕", dotum, "돋움", Arial, tahoma, verdana;
      line-height:1.5;color:#6f6f6f}

/* based */

.clearfix {*zoom:1}
.clearfix:after {content:"";display:block;clear:both;height:0}

.box-size {-webkit-box-sizing:border-box;-moz-box-sizing:border-box;box-sizing:border-box}

a:link, a:visited {color:#6f6f6f;text-decoration:none}, a:active, a:hover, a:focus {color:#8f8f8f}

::-moz-selection {color:#fff;background-color:#6fa8cb}
::selection {color:#fff;background-color:#6fa8cb}

.point-color01 {color:#e98900}

.point-color01 a:link {color:#e98900}
.point-color01 a:visited {color:#e98900}
.point-color01 a:hover {color:#e98900;text-decoration:underline}
.point-color01 a:focus {color:#e98900}
.point-color01 a:active {color:#e98900}

