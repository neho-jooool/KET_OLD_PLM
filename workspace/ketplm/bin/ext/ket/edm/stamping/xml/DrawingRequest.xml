      <stampType>APPROVAL</stampType>
      
      <!-- stamping 요청 승인일 : ex) 2014-07-02, Not Empty -->
      <stampAprovedDate>2014-07-28</stampAprovedDate>
      
      <!-- stamping 영문 부서명 : Not Empty -->
      <stampTeamName><![CDATA[R&D CENTER DEVELOPMENT 3 TEAM]]></stampTeamName>
      
      <!-- stamping 요청 : Y, N 중 하나,Not Empty -->
      <stampRequest>Y</stampRequest>
      
      <!-- step 요청 : Y, N 중 하나,Not Empty -->
      <stepRequest>N</stepRequest>
      
