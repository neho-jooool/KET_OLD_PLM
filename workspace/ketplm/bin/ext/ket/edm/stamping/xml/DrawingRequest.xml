  <!-- [Sheet명] : 2D도면 내의 Sheet의 명으로 한글은 제외되며, 영문은 소문자로 변환 -->
  
  <!-- 변환 Log 파일명 확장자 -->
  <stampLogFileExt>log</stampLogFileExt>
  <!-- 변환 에러 유형 로그 파일명 Suffix -->
  <stampErrFileSuffix>_e</stampErrFileSuffix>  
   
  <!-- ########################### -->
  <!-- ## Stamping 전역 공통      ## -->
  <!-- ########################### -->
  
  <!-- stamping 영문 회사명 : Not Empty -->
  <stampCompanyName><![CDATA[KOREA ELECTRIC TERMINAL CO., LTD.]]></stampCompanyName>
  
  
  <!-- ############################# -->
  <!-- ## 도면 배포 요청서 상세 정보 ## -->
  <!-- ############################# -->
  
  <!-- 도면 배포 요청서 배치 정보 -->  
  <requestDrawingList>
    
    <!-- ########################################################### -->
    <!-- ## 도면 정보 - NX 도면변환(Cadiance만 해당) 및 Stamping 예제 ## -->
    <!-- ########################################################### -->
    
    <!-- 도면 개별 정보 1건별 Drawing Tag 하나로 여러 도면 존재 가능함 -->
    <drawing>
    
      <!-- CadAppType : CREO, AUTOCAD, NX 3개 중 하나, Not Empty -->
      <cadType>NX</cadType>
    
      <!-- 2D 도면 변환 유무 : Y, N 중 하나, Not Empty -->
      <transRequest>Y</transRequest>
      
      <!-- id : 2D 포함 3D 확인용 object id -->
      <cadOid>15318445</cadOid>
      
      <!-- 도면 Number : 도면 Number, Not Empty -->
      <drawingNumber>23034000-L013_PRT</drawingNumber>
      
      <!-- 도면 Version : 도면 버전, Not Empty -->
      <drawingVersion>3.12</drawingVersion>
      
      <!-- 도면 CadFileName : 도면 파일 명, Not Empty -->
      <drawingFileName><![CDATA[23034000-L013.prt]]></drawingFileName>
      
      <!-- 2D 포함 3D CAD Number[참고용] : 3D번호, None 중 하나, Not Empty -->
      <transThreedCadNo>23034000-L013_PRT</transThreedCadNo>
     
      <!-- 2D 포함 3D CAD Oid[참고용] : Oid, None 중 하나, Not Empty -->
      <transThreedCadOid>test:1558</transThreedCadOid>
      
      <!-- 2D 포함 3D CAD Version[참고용] : 3D버전, None 중 하나, Not Empty -->
      <transThreedCadVersion>3.12</transThreedCadVersion>
      
      <!-- 2D 포함 3D CAD FileName[참고용] : 3D파일명, None 중 하나, Not Empty -->
      <transThreedCadFileName><![CDATA[23034000-L013.prt]]></transThreedCadFileName>
            
      <!-- NX 도면변환 관련 Active Sheet vs 전체 Sheet : Active, All, None 3개 중 하나, Not Empty -->
      <transNxSelectedSheet>Active</transNxSelectedSheet>
       
      <!-- stamping 영문 유형 (발송, 승인, 출도, 출도(가공수정), 검토 : ex) APPROVAL 등, Not Empty -->
      <stampType>APPROVAL</stampType>
      
      <!-- stamping 요청 승인일 : ex) 2014-07-02, Not Empty -->
      <stampAprovedDate>2014-07-02</stampAprovedDate>
            
      <!-- stamping 영문 부서명 : Not Empty -->
      <stampTeamName><![CDATA[R&D CENTER DEVELOPMENT 2 TEAM]]></stampTeamName>
      
      <!-- stamping 요청 : Y, N 중 하나,Not Empty -->
      <stampRequest>Y</stampRequest>
      
      <!-- step 요청 : Y, N 중 하나,Not Empty -->
      <stepRequest>N</stepRequest>
      
    </drawing>
    
    
    <!-- ############################################### -->
    <!-- ## 도면 정보 - CREO 도면변환 및 Stamping 예제   ## -->
    <!-- ############################################### -->
     
    <drawing>
    
      <!-- CadAppType : CREO, AUTOCAD, NX 3개 중 하나, Not Empty -->
      <cadType>CREO</cadType>
      
      <!-- 2D 도면 변환 유무 : Y, N 중 하나, Not Empty -->
      <transRequest>Y</transRequest>
      
      <!-- id : 도면 프로그램 확인용 object id -->
      <cadOid>56318963</cadOid>
      
      <!-- Number : 도면 Number, Not Empty -->
      <drawingNumber>MG675612-43_2D</drawingNumber>
      
      <!-- Version : 도면 버전, Not Empty -->
      <drawingVersion>0.5</drawingVersion>
      
      <!-- CadFileName : 도면 파일 명, Not Empty -->
      <drawingFileName><![CDATA[mg675612-43_060_12p_spacer.drw]]></drawingFileName>
            
      <!-- NX 도면변환 관련 Active Sheet vs 전체 Sheet : Active, All, None 3개 중 하나, Not Empty -->
      <transNxSelectedSheet>None</transNxSelectedSheet>
      
      <!-- 2D 연관 3D CAD Number[참고용] : 3D번호, None 중 하나, Not Empty -->
      <transThreedCadNo>MG675612-41_3D</transThreedCadNo>
      
      <!-- 2D 연관 3D CAD Version[참고용] : 3D버전, None 중 하나, Not Empty -->
      <transThreedCadVersion>0.10</transThreedCadVersion>
      
      <!-- 2D 포함 3D CAD Oid[참고용] : Oid, None 중 하나, Not Empty -->
      <transThreedCadOid>test:1559</transThreedCadOid>
      
      <!-- 2D 포함 3D CAD FileName[참고용] : 3D파일명, None 중 하나, Not Empty -->
      <transThreedCadFileName><![CDATA[mg675612-41_060_12p_spacer.prt]]></transThreedCadFileName>
      
      <!-- stamping 영문 유형 (발송, 승인, 출도, 출도(가공수정), 검토 : ex) APPROVAL 등, Not Empty -->
      <stampType>APPROVAL</stampType>
      
      <!-- stamping 요청 승인일 : ex) 2014-07-02, Not Empty -->
      <stampAprovedDate>2014-07-03</stampAprovedDate>
      
      <!-- stamping 영문 부서명 : Not Empty -->
      <stampTeamName><![CDATA[R&D CENTER DEVELOPMENT 1 TEAM]]></stampTeamName>
      
      <!-- stamping 요청 : Y, N 중 하나,Not Empty -->
      <stampRequest>Y</stampRequest>
      
      <!-- step 요청 : Y, N 중 하나,Not Empty -->
      <stepRequest>N</stepRequest>
      
    </drawing>
    
    
    <!-- ####################################### -->
    <!-- ## 도면 정보 - AutoCad Stamping 예제   ## -->
